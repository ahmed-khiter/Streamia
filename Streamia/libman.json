{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "@ttskch/select2-bootstrap4-theme@1.4.0",
      "destination": "wwwroot/ttskch/select2-bootstrap4-theme/",
      "files": [
        "dist/select2-bootstrap4.min.css"
      ]
    },
    {
      "library": "Swiper@4.4.6",
      "destination": "wwwroot/lib/Swiper/",
      "files": [
        "css/swiper.min.css",
        "js/swiper.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.5.2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "font-awesome@5.14.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/fontawesome.min.css"
      ]
    },
    {
      "library": "magnific-popup.js@1.1.0",
      "destination": "wwwroot/lib/magnific-popup.js/",
      "files": [
        "jquery.magnific-popup.min.js",
        "magnific-popup.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery.easing@1.3.2",
      "destination": "wwwroot/lib/jquery.easing/",
      "files": [
        "jquery.easing.min.js"
      ]
    }
  ]
}